#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/
    int i ,j,n,m,sum;
    
    sum=0;
    cin>>n>>m;
    
    int a[n][m];
    
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            cin>>a[i][j];
            
        }
    }
    for(j=0;j<m;j++){
        sum=0;
    	for(i=0;i<n;i++){
            sum=sum+a[i][j];
        }
       cout<<sum<<' ';
    }
    
  
}
